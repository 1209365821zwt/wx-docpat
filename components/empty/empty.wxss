/* components/empty/empty.wxss */
.empty{
  width: 100%;
  height: 100%;
  min-height: 400rpx;
  display: flex;
  justify-content: center;
  position: relative;
  text-align: center;
}
.empty_content{
  position: absolute;
  top: calc(50% - 64rpx);
}
.empty .empty_image{
  width: 200rpx;
  height: 128rpx;
}
.empty .empty_text{
  color: #A0A0A0;
  font-size: 24rpx;
  margin-top: 28rpx;
}