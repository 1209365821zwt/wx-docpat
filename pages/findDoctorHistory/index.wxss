/* pages/findDoctorHistory/index.wxss */
.doctor-history{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.search-wrapper{
  width: 100%;
}
.search-wrapper .btn-box .btn-box-in{
  display: flex;
}
.search-wrapper .btn-box .btn-box-in .history{
  flex: 1;
  margin: 40rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(37,58,82,1);
  line-height:28rpx;
}
.search-wrapper .btn-box .btn-box-in .cancel{
  width: 28rpx;
  height: 28rpx;
  padding-right: 26rpx;
  padding-top: 30rpx;
}
.searchList{
  display: flex;
  flex-wrap: wrap;
}
.searchList .search{
  background:rgba(253,252,252,1);
  border-radius:8rpx;
  border:1px solid rgba(37,42,54,0.2);
  padding: 16rpx 32rpx;
  margin: 15rpx 32rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(37,42,54,0.9);
  background: #FDFCFC;
}
.van-search .van-search__action{
  color:#009FDA;
  font-size: 32rpx;
  
}